



@model StellerAcunMedyaAkademiPortfolio.Models.TBL_SocialMedia
@{
    ViewBag.Title = "AddSocialMedia";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>Yeni Sosyal Medya Ekleme Sayfası</h3>

<html>
<head>
    <title>CSHTML'e CSS Ekleme</title>
    <link rel="stylesheet" type="text/css" href="~/Content/StyleSheet1.css">
</head>
<body>





    <form method="post">



        <input id="input" type="text" required name="SocialMediaName" />
        <label for="input" alt="Sosyal Medya Adı Giriniz" placeholder="Sosyal Medya Adı "></label>

        <br />
        <input id="input" type="text" required name="Url" />
        <label for="input" alt="Sosyal Medya Linkini Giriniz" placeholder="Sosyal Medya Linki "></label>
        <br />
        <input id="input" type="text" required name="Icon" />
        <label for="input" alt="Sosyal Medya İkonu Giriniz" placeholder="Sosyal Medya İkonu "></label>
        <br />





        <button class="btn btn-outline-primary">Kaydet</button>

    </form>

</body>
</html>